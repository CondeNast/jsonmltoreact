{
  "name": "@condenast/jsonmltoreact",
  "version": "0.1.1",
  "description": "JsonML to React component converter",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run build:js",
    "build:js": "./node_modules/.bin/babel ./src --out-dir ./lib",
    "coverage": "nyc --reporter=lcov --require babel-core/register mocha ${TEST_FILES:-test/src/index.spec.js}",
    "lint": "eslint --ext .js -c .eslintrc .",
    "prebuild": "rm -rf lib",
    "precommit": "npm run lint",
    "prepublish": "npm run build",
    "prepush": "npm test",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CondeNast/jsonmltoreact"
  },
  "license": "MIT",
  "dependencies": {
    "jsonml.js": "^0.1.0",
    "lodash": "^4.14.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-rewire": "^1.0.0-rc-3",
    "babel-preset-es2015": "^6.6.0",
    "babel-template": "^6.8.0",
    "babel-types": "^6.8.1",
    "chai": "^3.5.0",
    "eslint": "^2.10.1",
    "husky": "^0.11.4",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4",
    "sinon": "^1.17.4"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ]
  }
}
